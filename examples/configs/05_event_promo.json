      {
        "type": "image",
        "name": "venue_photo",
        "path": "C:/AE_Projects/assets/venue.jpg"
      },
      {
        "type": "image",
        "name": "speaker_photo",
        "path": "C:/AE_Projects/assets/speaker.jpg"

        {
          "change_type": "swap_items_by_index",
          "layer_name": "event_logo",
          "comp_name": "EventOpener",
          "layer_index": "3",
          "fit_to_screen": false,
          "fit_to_screen_width": true,
          "fit_to_screen_height": false

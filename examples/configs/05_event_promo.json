      {
        "change_type": "update_layer_property",
        "comp_name": "InfoTemplate",
        "layer_name": "InfoTitle",
        "property_name": "Text.Source Text",
        "property_type": "string",
        "value": "{title}"
      },
      {
        "change_type": "update_layer_property",
        "comp_name": "InfoTemplate",
        "layer_name": "InfoDetails",
        "property_name": "Text.Source Text",
        "property_type": "string",
        "value": "{details}"
      }
    ]
  },
